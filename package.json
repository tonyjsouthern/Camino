{
  "name": "Camino",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonyjsouthern/Camino.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dist": "build",
    "build": "build --win",
    "ship": "build --win -p always",
    "deploy": "electron-builder build --win --publish always"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "electron": "^4.2.0",
    "electron-builder": "^20.40.2",
    "electron-rebuild": "^1.8.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^6.2.0",
    "electron-builder-squirrel-windows": "^20.41.0",
    "electron-updater": "^4.0.6",
    "mssql": "^6.0.1",
    "sequelize": "^5.15.1",
    "socket.io": "^2.2.0",
    "systemjs": "^6.1.7",
    "tedious": "^3.0.1"
  },
  "build": {
    "appId": "Camino",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "icon.ico"
    }
  }
}
